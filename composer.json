{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "laravel/cashier": "~6.0",
        "pda/pheanstalk": "3.1.*",
        "cviebrock/eloquent-sluggable": "~4.1.2",
        "morrislaptop/laravel-queue-clear": "^1.0",
        "barryvdh/laravel-dompdf": "^0.6.1",
        "barryvdh/laravel-snappy": "^0.3.0",
        "danielboendergaard/phantom-pdf": "^1.2",
        "barryvdh/laravel-debugbar": "^2.2",
        "cmgmyr/messenger": "^2.13",
        "orchestra/parser": "~3.2",
        "fideloper/proxy": "^3.3",
        "tarsana/functional": "^2.2",
        "joshralph/password-policy": "dev-master",
        "symfony/polyfill-php73": "^1.10",
        "doctrine/dbal": "~2.9",
        "aws/aws-sdk-php": "~3.86",
        "aws/aws-sdk-php-laravel": "~3.3",
        "guzzlehttp/guzzle": "~6.3",
        "predis/predis": "~1.1",
        "components/font-awesome": "4.5.0",
        "twbs/bootstrap": "3.3.7"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
